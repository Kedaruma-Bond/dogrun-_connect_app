<%= render_turbo_stream_flash_messages %>

<%= turbo_stream.replace "entry_#{@entry.id}" do %>
  <%= render partial: "entry", locals: { entry: @entry } %>
<% end %>

<%= turbo_stream.update dom_id(@dogrun_place, "admin_num_of_playing_dogs") do %>
  <%= render partial: "num_of_playing_dogs", locals: { num_of_playing_dogs: @num_of_playing_dogs } %>
<% end %>

<%= turbo_stream.replace "registration_number_#{@entry.registration_number.id}" do %>
  <%= render partial: "admin/dogs/dog", locals: { dog: @entry.dog } %>
<% end %>
